{
  "family": "caju-front",
  "containerDefinitions": [
    {
      "name": "caju-container",
      "image": "851725231124.dkr.ecr.us-east-1.amazonaws.com/caju:90381e1fe840340343516b3cd6851983f2fcb029",
      "cpu": 0,
      "portMappings": [
        {
          "name": "caju-container-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "entryPoint": ["yarn preview"],
      "environment": [
        {
          "name": "VITE_HOST_API",
          "value": "http://localhost:8000"
        },
        {
          "name": "localhost:8000",
          "value": ""
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "healthCheck": {
        "command": ["CMD-SHELL", "curl -f http://localhost/ || exit 1"],
        "interval": 30,
        "timeout": 5,
        "retries": 5,
        "startPeriod": 30
      },
      "systemControls": []
    }
  ],
  "taskRoleArn": "arn:aws:iam::851725231124:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::851725231124:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["EC2"],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
