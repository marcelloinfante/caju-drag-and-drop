{
  "family": "caju-front",
  "containerDefinitions": [
    {
      "name": "caju-container",
      "image": "851725231124.dkr.ecr.us-east-1.amazonaws.com/caju",
      "cpu": 0,
      "portMappings": [
        {
          "name": "caju-container-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "entryPoint": ["yarn preview"],
      "environment": [
        {
          "name": "localhost:8000",
          "value": ""
        }
      ],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "systemControls": []
    }
  ],
  "taskRoleArn": "arn:aws:iam::851725231124:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::851725231124:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["EC2"],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
